@model Contato

@{
    ViewData["Title"] = "Confirmacao apagar contato";
}
<div class="text-center">
    <h3>Realmente deseja apagar o contato(@Model.Nome)</h3>
</div>
<br />
<a class="btn btn-primary btn-block" role="button" asp-controller="Contato" asp-action="Index">Cancelar Exclusão</a>
<a class="btn btn-danger btn-block" role="button"asp-controller="Contato" asp-action="Apague" asp-route-id="@Model.ContatoId">Confirmar Exclusão</a>

